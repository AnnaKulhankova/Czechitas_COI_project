--------------------------------------------------------------------------------------------------------------
--SANKCE
-- vytvarim pomocnou tabulku SANKCE z původní tabulky COI_sankce
-- duplicity osetrim pomocí prikazu DISTINCT "Id_Sankce"
-- datum prevadim na univerzalni format
--ciselne hodnoty prevadime na INT

CREATE TABLE SANKCE AS
SELECT
    DISTINCT S."Id_Sankce"::INT AS "Id_Sankce"
    , S."ID_kontroly"::INT AS "ID_kontroly"
    , S."Vyse_pokuty"::INT AS "Vyse_pokuty"
    , S."Zakon"
    , S."S" 
    , U."URL" AS "URL_sankce"
    , TO_DATE(S."Datum_nabyti_Pravni_Moci", 'dd.mm.yyyy') as "Datum_nabyti_pravni_moci"
FROM "COI_sankce" AS S
LEFT JOIN "url-sankce-list-1" AS U
ON S."Zakon" = U."Cislo";

--odmazavam radky, ktere se nenapoji na kontroly
DELETE FROM SANKCE 
WHERE "ID_kontroly" IN (SELECT "ID_kontroly" 
FROM KONTROLY AS K
FULL OUTER JOIN SANKCE AS S
ON K."Id_kontroly" = S."ID_kontroly"
WHERE "Id_kontroly" IS NULL);

-----------------------------------------------------------------------
--TABULKA ZAKAZY

CREATE OR REPLACE TABLE ZAKAZY AS
SELECT
    DISTINCT Z."Id_zakazu"::INT AS "Id_zakazu"
    , Z."ID_kontroly" ::INT AS "ID_kontroly"
    , Z."Sortiment"
    , CASE
        WHEN Z."Mnozstvi" <> '' THEN (REPLACE(Z."Mnozstvi", ';', '.'))::FLOAT
        ELSE NULL
       END AS "Mnozstvi"
    , Z."Jednotka"
    , CASE
        WHEN Z."Cena_celkem" <> '' THEN (REPLACE(Z."Cena_celkem", ';', '.'))::FLOAT
        ELSE NULL
       END AS "Cena_celkem"
    , Z."Dle_zakona"
    , U."URL" AS "URL_zakazy"
FROM "COI_zakazy" AS Z
LEFT JOIN "url-zakazy-list-1" AS U
ON Z."Dle_zakona" = U."Cislo";

--opravuji jeden radek, kde cena zaporna
UPDATE ZAKAZY SET
    "Cena_celkem" = 0
WHERE "Cena_celkem" < 0;

--odmazavam radky, ktere se nenapoji na kontroly
DELETE FROM ZAKAZY 
WHERE "ID_kontroly" IN (SELECT "ID_kontroly" 
FROM KONTROLY AS K
FULL OUTER JOIN ZAKAZY AS Z
ON K."Id_kontroly" = Z."ID_kontroly"
WHERE "Id_kontroly" IS NULL);


-----------------------------------------------------------------------
CREATE or REPLACE TABLE DATA_CLEAN AS
SELECT
    K."Id_kontroly"
    ,K."Datum_kontroly"
    ,K."IC_subjektu"
    ,K."Firma"
    ,K."Forma_subjektu"
--    ,K."Datum_vzniku_firmy"
--    ,K."Rok_vzniku_firmy"
--   ,K."Datum_zaniku_firmy"
    ,K."Kraj"
--    ,K."Okres"
    ,K."Obec"
    ,K."Obec2"
    ,K."Adresa"
    ,CASE
    		WHEN K."PSC" <> '' THEN K."PSC"::INT
        ELSE NULL
      END AS "PSC"
--    ,K."Zeme"
    ,S."Id_Sankce"
    ,S."Vyse_pokuty"
    ,S."Zakon"
    ,S."S"
    ,S."URL_sankce"
    ,S."Datum_nabyti_pravni_moci"
    ,Z."Id_zakazu"
    ,Z."Sortiment"
    ,Z."Mnozstvi"
    ,Z."Jednotka"
    ,Z."Cena_celkem"
    ,Z."Dle_zakona"
    ,Z."URL_zakazy"
FROM KONTROLY AS K
LEFT JOIN SANKCE AS S
ON K."Id_kontroly" = S."ID_kontroly"
LEFT JOIN ZAKAZY AS Z
ON K."Id_kontroly" = Z."ID_kontroly";

DELETE FROM DATA_CLEAN
WHERE "IC_subjektu" IS NULL AND "Kraj" ILIKE 'Neznám%' AND "Id_Sankce" IS NULL AND "Id_zakazu" IS NULL;

