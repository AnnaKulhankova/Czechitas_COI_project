select * from "COI_zajisteni";

CREATE TABLE "Zajisteni_skusobna" AS SELECT
 "Id_Zajisteni":: INT as "Id_Zajisteni"
, "ID_kontroly"::INT as "ID_kontroly"
, "Znacka" 
, "Vyrobek"
, "Mnozstvi"::INT as "Mnozstvi"
FROM "COI_zajisteni";

select * from "Zajisteni_skusobna";

